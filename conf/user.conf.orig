#
#############################################


# general variables
lumikki_prefix="/home/jko/usr"
lumikki_config_path="/home/jko/workspace/lumikki/lumikki/conf"

# memcache configurations
lumikki_memcache_ip="127.0.0.1"
lumikki_memcache_port="38338" 

# cgi python environment
lumikki_python_pythonpath="/home/jko/usr/lib/python2.6/site-packages"
lumikki_python_cgiassign="/usr/bin/python2.6"
lumikki_python_uidefaults="/home/jko/workspace/lumikki/lumikki/conf/uiDefaults.conf"

# lighttpd conf
lumikki_lighttpd_port="8000"
lumikki_lighttpd_log_root="/home/jko/workspace/lumikki/lumikki/tmp"
lumikki_lighttpd_state_dir="/home/jko/workspace/lumikki/lumikki/tmp"
lumikki_lighttpd_server_root="/home/jko/workspace/lumikki/lumikki/webroot"
